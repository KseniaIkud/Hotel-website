nSans;\n        weight: 700;\n        size: 24px;\n    }\n    color: $dark-shade-100-color;\n    line-height: 30px;\n}\n\n@mixin h2-text {\n    font: {\n        family: Quicksand, OpenSans;\n        weight: 700;\n        size: 19px;\n    }\n    color: $dark-shade-100-color;\n    line-height: 24px;\n}\n\n@mixin h3-text {\n    font: {\n        family: Montserrat;\n        weight: 700;\n        size: 12px;\n    }\n    color: $dark-shade-100-color;\n    line-height: 15px;\n    text-transform: uppercase;\n}\n\n@mixin body-text {\n    font: {\n        family: Montserrat;\n        weight: normal;\n        size: 14px;\n    }\n    color: $dark-shade-75-color;\n    line-height: 24px;\n}\n\n//nulling styles\n* {\n    margin: 0;\n    padding: 0;\n    border: none;\n    list-style: none;\n}\n\n*,*:before,*:after{\n\t-moz-box-sizing: border-box;\n\t-webkit-box-sizing: border-box;\n\tbox-sizing: border-box;\n}\n:focus,:active{\n    outline: none;\n}\n\na:focus,a:active{\n    outline: none;\n}\n\nnav,footer,header,aside{\n    display: block;\n}\n\nhtml,body{\n\theight: 100%;\n\twidth: 100%;\n\tfont-size: 100%;\n\tline-height: 1;\n\tfont-size: 14px;\n\t-ms-text-size-adjust: 100%;\n\t-moz-text-size-adjust: 100%;\n\t-webkit-text-size-adjust: 100%;\n}\ninput,button,textarea{\n    font-family:inherit;\n}\n\ninput::-ms-clear{\n    display: none;\n}\n\nbutton{\n    cursor: pointer;\n}\n\nbutton::-moz-focus-inner {\n    padding:0;border:0;\n}\n\na, a:visited {\n    text-decoration: none;\n    color: inherit;\n}\n\nh1, h2, h3, h4, h5, h6 {\n    font-size: inherit;\n    font-weight: normal;\n}\n\n\n","first_header.scss":"@import '../../../common/common-styles.scss';\n\n//fonts\n@font-face {\n\tfont-family: 'Montserrat';\n\tsrc: url('fonts/Montserrat-Regular.eot');\n\tsrc: local('☺'), url('fonts/Montserrat-Regular.woff') format('woff'), url('fonts/Montserrat-Regular.ttf') format('truetype'), url('fonts/Montserrat-Regular.svg') format('svg');\n\tfont-weight: normal;\n\tfont-style: normal;\n}\n\n@font-face {\n\tfont-family: 'Montserrat';\n\tsrc: url('fonts/Montserrat-Bold.eot');\n\tsrc: local('☺'), url('fonts/Montserrat-Bold.woff') format('woff'), url('fonts/Montserrat-Bold.ttf') format('truetype'), url('fonts/Montserrat-Bold.svg') format('svg');\n\tfont-weight: 700;\n\tfont-style: normal;\n}\n\n//styles\n.first-header {\n\t@include body-text;\n\tfont-size: 14px;\n\tline-height: 17px;\n\twidth: 1440px;\n\theight: 70px;\n\tmargin: auto;\n\tbackground-color: #FFFFFF;\n\tbox-shadow: 0px 10px 20px rgba(31, 32, 65, 0.05);\n\t&__content{\n\t\twidth: 1160px;\n\t\theight: 70px;\n\t\tmargin: auto;\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tjustify-content: space-between;\n\t\t\n\t}\n}\n.main-menu {\n\tdisplay: flex;\n\tjustify-content: space-between;\n\twidth: 757px;\n\t\n\t&__list{\n\t\twidth: 530px;\n\t\t\n\t\tdisplay: flex;\n\t\tjustify-content: space-between;\n\t}\n\t&__link {\n\t\t&_main {\n\t\t\tfont-weight: 700;\n\t\t}\n\t\t&_arrow {\n\t\t\tposition: relative;\n\t\t\t\n\t\t\t.arrow {\n\t\t\t\tposition: absolute;\n\t\t\t\tmargin: 5px;\n\t\t\t}\n\t\t\t\n\t\t}\n\t}\n\t&__button {\n\t\twidth: 200px;\n\t\tdisplay: flex;\n\t\tjustify-content: space-between;\n\t}\n\t&__login {\n\t\tcolor: red;\n\t}\n\t&__register {\n\t\tcolor:green;\n\t}\n}\n\n.test {\n\tfont-size: 14px;\n}\n\n"},"lineCount":null}},"error":null,"hash":"b1a3b16b5f92aa0fbd6e7a5e1aa23599","cacheData":{"env":{}}}